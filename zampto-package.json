{
  "name": "sing-box-zampto",
  "version": "1.0.0",
  "description": "Optimized sing-box service for zampto Node.js platform (Node10, ARM)",
  "main": "index.js",
  "bin": {
    "sing-box-start": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "stop": "pkill -f 'node index.js'",
    "restart": "npm run stop && npm start",
    "logs": "tail -f logs/*.log"
  },
  "keywords": [
    "sing-box",
    "vmess",
    "proxy",
    "argo",
    "cloudflare",
    "zampto",
    "node",
    "arm"
  ],
  "author": "zampto optimization team",
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eooce/Auto-deploy-sap-and-keepalive.git"
  },
  "dependencies": {},
  "devDependencies": {},
  "optionalDependencies": {},
  "preferGlobal": false,
  "private": true,
  "config": {
    "port": 3000,
    "cpu_optimization": "40-50%"
  },
  "environment": {
    "required": [
      "UUID"
    ],
    "optional": [
      "NAME",
      "SERVER_PORT",
      "FILE_PATH",
      "SUB_PATH",
      "ARGO_DOMAIN",
      "ARGO_AUTH",
      "ARGO_PORT",
      "CFIP",
      "CFPORT",
      "NEZHA_SERVER",
      "NEZHA_PORT",
      "NEZHA_KEY",
      "CHAT_ID",
      "BOT_TOKEN",
      "UPLOAD_URL"
    ]
  }
}
